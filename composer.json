{
    "name": "vresh/twilio-bundle",
    "type": "symfony-bundle",
    "description": "A simple Symfony2 bundle for the official sdk provided by Twilio.",
    "keywords": ["Twilio"],
    "homepage": "https://github.com/fridolin-koch/VreshTwilioBundle",
    "license": "MIT",
    "version": "0.4.3",
    "authors": [
        {
            "name": "Fridolin Koch",
            "email": "info@fridokoch.de"
        }
    ],
    "minimum-stability": "dev",
    "repositories": [
        {"type": "vcs", "url": "git@github.com:novatex/twilio-php.git"}
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": ">=2.1",
        "twilio/sdk": "dev-3.12.8-logged"
    },
    "require-dev": {
      "phpunit/phpunit": "~4.1"
    },
    "autoload": {
        "psr-0": { "Vresh\\TwilioBundle": "" }
    },
    "target-dir": "Vresh/TwilioBundle"
}
